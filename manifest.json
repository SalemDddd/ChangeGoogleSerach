{
	"manifest_version": 3,
	"name": "Change search?",
	"description": "Расширение меняет запрос на заведомо верный",
	"version": "0.0.1",
	"author": "SalemD",
	"browser_action": {
		"default_title": "By SalemDev",
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.google.com/*"
			],
			"js": ["content.js"]
		}
	],
	"permissions": ["activeTab"]
}